{
  "name": "react-redux-boilerplate",
  "version": "0.0.1",
  "description": "React and Redux Boilerplate with best practices based on latest trends / technologies",
  "scripts": {
    "start": "PUBLIC_URL=http://localhost:3001 react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install && npx husky add .husky/pre-commit \"npx lint-staged\""
  },
  "keywords": [
    "react",
    "react.js",
    "redux",
    "redux.js",
    "boilerplate",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "eslint",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micchyboy237/react-redux-boilerplate.git"
  },
  "author": "Jethro Estrada <jethroestrada237@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micchyboy237/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/micchyboy237/react-redux-boilerplate#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
